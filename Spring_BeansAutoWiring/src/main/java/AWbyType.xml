<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation = "http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean id="BeanAutoWireByType" class="com.SpringFramework.Spring_BeansAutoWiring.BeanAutoWireByType" autowire="byType"/>
	
	<bean id="Point" class="com.SpringFramework.Spring_BeansAutoWiring.Points">
		<property name="X" value="100"/>
		<property name="Y" value="200"/>
		<property name="Z" value="300"/>
	
	 </bean>


</beans>